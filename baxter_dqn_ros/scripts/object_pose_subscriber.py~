#!/usr/bin/env python

#import roslib; roslib.load_manifest('gazebo')
import sys
import rospy
from gazebo_msgs.msg import ModelStates

def callback(data):
	index = data.name.index('block')
	object_pose = data.pose[index]
	

def object_pose_listener():
	rospy.init_node('object__pose_listener')
	rospy.Subscriber("/gazebo/model_states", ModelStates, callback)
	rospy.spin()

if __name__ == '__main__':
	object_pose_listener()
